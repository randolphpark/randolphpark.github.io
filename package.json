{
  "name": "react-static-example-blank",
  "private": true,
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "test": "echo 'no test yet'",
    "build": "react-static build",
    "serve": "serve dist -p 3000 -s",
    "format": "prettier --write '{src/**/*,src/*}.js'",
    "prettier": "prettier --list-different '{src/**/*, src/*}.js'",
    "lint": "eslint '{src/*, src/**/*}.js'",
    "lint-fix": "eslint --fix '{src/**/*, src/*}.js'",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test && lint-staged && yarn prettier"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint-fix",
      "yarn format",
      "git add"
    ]
  },
  "dependencies": {
    "@octokit/rest": "^16.28.6",
    "@reach/router": "^1.2.1",
    "await-to-js": "^2.1.1",
    "axios": "^0.19.0",
    "core-js": "3",
    "gray-matter": "^4.0.1",
    "i": "^0.3.6",
    "klaw": "^3.0.0",
    "lint-staged": "^8.1.5",
    "moment": "^2.20.1",
    "npm": "^6.9.0",
    "react": "^16.8.6",
    "react-bulma-components": "2.3.0",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.2",
    "react-markdown": "^4.1.0",
    "react-moment": "^0.9.2",
    "react-static": "^6.3.9",
    "react-static-plugin-source-filesystem": "^7.1.0",
    "recharts": "^1.5.0",
    "snyk": "^1.401.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.15.3",
    "eslint-config-react-app": "^3.0.8",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-flowtype": "^3.10.4",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.1",
    "prettier": "^1.18.2",
    "serve": "^11.0.0"
  },
  "browserslist": [
    "defaults"
  ],
  "snyk": true
}
