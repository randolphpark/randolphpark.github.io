{
  "name": "react-static-example-blank",
  "private": true,
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "test": "echo 'no test yet'",
    "build": "react-static build",
    "serve": "serve dist -p 3000 -s",
    "format": "prettier --write '{src/**/*,src/*}.js'",
    "prettier": "prettier --list-different '{src/**/*, src/*}.js'",
    "lint": "eslint '{src/*, src/**/*}.js'",
    "lint-fix": "eslint --fix '{src/**/*, src/*}.js'"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test && lint-staged && yarn prettier"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint-fix",
      "yarn format",
      "git add"
    ]
  },
  "dependencies": {
    "@octokit/rest": "^16.15.0",
    "axios": "^0.18.0",
    "gray-matter": "^4.0.1",
    "i": "^0.3.6",
    "klaw": "^3.0.0",
    "lint-staged": "^8.1.3",
    "moment": "^2.20.1",
    "npm": "^6.7.0",
    "react": "^16.8.1",
    "react-bulma-components": "2.3.0",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.6.5",
    "react-markdown": "^4.0.3",
    "react-moment": "^0.8.4",
    "react-static": "^6.3.5",
    "recharts": "^1.4.3",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.13.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "serve": "^10.1.2"
  }
}
